	<command:command>
		<command:details>
			<command:name>Get-RemoteDesktopConfig</command:name>
			<maml:description>
				<maml:para>Returns a string indicating the state of remote desktop.</maml:para>
			</maml:description>
			<maml:copyright>
				<maml:para>(c) Microsoft Corporation 2009</maml:para>
			</maml:copyright>
			<command:verb>Get</command:verb>
			<command:noun>RemoteDesktopConfig</command:noun>
			<dev:version>1.00</dev:version>
		</command:details>
		<maml:description>
			<maml:para>Remote Desktop can be configured to one of three state:
"Connections not allowed" , "Only Secure Connections allowed", 
"All Connections allowed"</maml:para>
		</maml:description>
		<command:syntax>
			<command:syntaxItem>
				<maml:name>Get-RemoteDesktopConfig</maml:name>
				
			</command:syntaxItem>
		</command:syntax>
		<command:parameters>
			
		</command:parameters>
		<command:inputTypes>
			<command:inputType>
				<dev:type>
					<maml:name></maml:name>
					<maml:uri></maml:uri>
					<maml:description>
						<maml:para></maml:para>
					</maml:description>
				</dev:type>
				<maml:description>
					<maml:para></maml:para>
				</maml:description>
			</command:inputType>
		</command:inputTypes>
		<command:returnValues>
			<command:returnValue>
				<dev:type>
					<maml:name>String</maml:name>
					<maml:uri></maml:uri>
					<maml:description>
						<maml:para></maml:para>
					</maml:description>
				</dev:type>
				<maml:description>
					<maml:para>The state of remote desktop </maml:para>
				</maml:description>
			</command:returnValue>
		</command:returnValues>
		<maml:alertSet>
			<maml:title></maml:title>
			
		</maml:alertSet>
